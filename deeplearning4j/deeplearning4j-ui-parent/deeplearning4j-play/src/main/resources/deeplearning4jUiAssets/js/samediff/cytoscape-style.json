[
  {
    "selector": ".uiop",
    "style": {
      "label": "data(label)",
      "width": 30,
      "height": 15,
      "text-valign": "center",
      "text-halign": "center",
      "text-wrap": "wrap",
      "font-size":6,
      "shape": "roundrectangle"
    }
  },
  {
    "selector": ".uivariable",
    "style": {
      "label": "data(label)",
      "width": 6,
      "height": 6,
      "text-valign": "center",
      "text-halign": "left",
      "text-wrap": "wrap",
      "font-size":5,
      "text-margin-x": -4
    }
  },
  {
    "selector": "edge",
    "style": {
      "curve-style": "bezier",
      "width": 1.5,
      "line-color": "#ccc",
      "target-arrow-shape": "triangle",
      "source-arrow-shape": "none",
      "source-arrow-color": "#A9A9A9",
      "target-arrow-color": "#A9A9A9",
      "arrow-scale": 0.7,
      "text-wrap": "wrap"
    }
  },
  {
    "selector": ".opoutputedge",
    "style": {
      "curve-style": "bezier",
      "width": 1.5,
      "line-color": "#ccc",
      "target-arrow-shape": "triangle",
      "source-arrow-shape": "none",
      "source-arrow-color": "#A9A9A9",
      "target-arrow-color": "#A9A9A9",
      "arrow-scale": 0.7,
      "text-wrap": "wrap"
    }
  },
  {
    "selector": ".placeholder",
    "style": {
      "background-color": "#d9d9d9",
      "border-width": 1,
      "border-color": "#000000"
    }
  },
  {
    "selector": ".variable",
    "style": {
      "background-color": "#b3e6ff"
    }
  },
  {
    "selector": ".constant",
    "style": {
      "background-color": "#d6f5d6"
    }
  }
]