#include <ops/impl/specials_single.hpp> // Original include
#include <system/type_boilerplate.h> // For type lists like SD_NUMERIC_TYPES_PART_X
#include <loops/pairwise_instantiations.h> // For ITERATE_COMBINATIONS macro
// Note: Instantiations are generated to prevent compiler memory issues

namespace sd {

// Instantiation for SpecialMethods class template
// Assumes ITERATE_COMBINATIONS handles single-parameter templates
// by using the first type from the generated pair based on partitions @COMB1@ and @COMB2@.
ITERATE_COMBINATIONS(
   SD_NUMERIC_TYPES_PART_@COMB1@,
   SD_NUMERIC_TYPES_PART_@COMB2@, // Second list needed by macro, but likely only Type1 is used
   template class sd::SpecialMethods, // Template pattern for the class (expects one type arg)
   SpecialMethods,                  // Base name (used internally by macro/CMake)
   ;                                // Trailing semicolon for the instantiation
)

} // namespace sd