#include <loops/cpu/broadcasting.hpp>
#include <loops/pairwise_instantiations.h>

// Standardized generation using partitioned types
// This template generates instantiations for integer broadcasting operations

namespace functions {
    namespace broadcast {
        // Use the standardized combination system for triple template instantiation
        ITERATE_COMBINATIONS_3(
            SD_INTEGER_TYPES_PART_@COMB1@,
            SD_INTEGER_TYPES,
            SD_INTEGER_TYPES_PART_@COMB2@,
            BUILD_TRIPLE_TEMPLATE,
            Broadcast,
            ;
        )
    }
}
