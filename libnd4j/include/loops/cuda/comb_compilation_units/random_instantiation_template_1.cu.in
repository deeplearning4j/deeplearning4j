/*
 * Function Instantiation:
 * RandomFunction::executeCudaSingle instantiated for types in @COMB1@
 * Note: Using @COMB1@ for single type (1-type combinations)
 */

#define LIST_CALLBACK_EXECUTE_CUDA_SINGLE(INPUT) \
    template void functions::random::RandomFunction<GET_SECOND(INPUT)>::executeCudaSingle( \
        dim3& launchDims, \
        cudaStream_t* stream, \
        int opNum, \
        sd::Pointer stateHost, \
        void* vz, \
        sd::LongType const* zShapeBuffer, \
        void* vextraArguments);

ITERATE_LIST((SD_COMMON_TYPES_PART_@COMB1@), LIST_CALLBACK_EXECUTE_CUDA_SINGLE)

#define LIST_CALLBACK_EXECUTE_CUDA_DOUBLE(INPUT) \
