ITERATE_LIST((SD_COMMON_TYPES_PART_@COMB1@), LIST_CALLBACK_EXECUTE_CUDA_DOUBLE)

#define LIST_CALLBACK_EXECUTE_CUDA_TRIPLE(INPUT) \
    template void functions::random::RandomFunction<GET_SECOND(INPUT)>::executeCudaTriple( \
